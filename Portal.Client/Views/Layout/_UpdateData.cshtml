
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Dashboard</title>

    <link rel="stylesheet" href="~/voler-master/dist/assets/css/bootstrap.css">

    <link rel="stylesheet" href="~/voler-master/dist/assets/vendors/chartjs/Chart.min.css">

    <link rel="stylesheet" href="~/voler-master/dist/assets/vendors/perfect-scrollbar/perfect-scrollbar.css">
    <link rel="stylesheet" href="~/voler-master/dist/assets/css/app.css">
    <link rel="shortcut icon" href="~/PortalClient/app/images/mtdl-logo.png" type="image/x-icon">

    <!-- Select2 -->
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-beta.1/dist/css/select2.min.css" rel="stylesheet" />
    @*<script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-beta.1/dist/js/select2.min.js"></script>*@



    <!-- Portal Style -->
    <link rel="stylesheet" href="~/PortalClient/app/scss/style.css">
</head>
<body>
    <div id="app">
        <div id="sidebar" class='active'>
            <div class="sidebar-wrapper active">

                <div class="sidebar-header">
                    <img src="~/metrodata.png" height="300" alt="" srcset="">
                </div>

                <div class="sidebar-menu">
                    <ul class="menu">


                        <li class='sidebar-title'>Main Menu</li>

                        <!-- Update / Edit Data -->
                        <li class="sidebar-item hide ">
                            <a href="@Url.Action("Update", "Account")" class='sidebar-link'>
                                <i data-feather="user" width="20"></i>
                                <span>Update / Edit Data</span>
                            </a>
                        </li>
                    </ul>
                </div>
                <button class="sidebar-toggler btn x"><i data-feather="x"></i></button>
            </div>
        </div>
        @RenderBody();
    </div>
    @RenderSection("script", required: false);
</body>
</html>

